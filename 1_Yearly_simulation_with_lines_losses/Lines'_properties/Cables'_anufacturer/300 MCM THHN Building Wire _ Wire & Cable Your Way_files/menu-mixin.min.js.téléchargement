define(['jquery',],function($){'use strict';return function(target){$.widget('mage.menu',target.menu,{_setActiveMenuForCategory:function(url){var self=this;var activeCategoryLink=this.element.find('a[href="'+url+'"]'),classes,classNav;if(!activeCategoryLink||!activeCategoryLink.hasClass('ui-corner-all')){return false;}else if(!activeCategoryLink.parent().hasClass('active')){$.each(activeCategoryLink,function(index){var currentActiveCategoryLink=$(activeCategoryLink[index]);currentActiveCategoryLink.parent().addClass('active');classes=currentActiveCategoryLink.parent().attr('class');classNav=classes.match(/(nav\-)[0-9]+(\-[0-9]+)+/gi);if(classNav){self._setActiveParent(classNav[0]);}});}
return true;},_setActiveParent:function(childClassName){var parentElement,parentClass=childClassName.substr(0,childClassName.lastIndexOf('-'));if(parentClass!=='nav-1'&&parentClass.lastIndexOf('-')!==-1){parentElement=this.element.find('.'+parentClass);if(parentElement){parentElement.addClass('has-active');}
this._setActiveParent(parentClass);}},});return{menu:$.mage.menu};}});